<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="401.0" prefWidth="880.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="juniter.service.adminfx.include.PeerPanel">
    <children>
        <VBox fx:id="identityForm1" prefHeight="200.0" prefWidth="100.0">
            <children>
                <HBox spacing="20.0">
                    <children>
                        <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Version"/>
                        <TextField fx:id="version" disable="true" editable="false" text="10"/>
                    </children>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                </HBox>
                <HBox spacing="20.0">
                    <children>
                        <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Type"/>
                        <TextField fx:id="type" disable="true" editable="false" text="Peer"/>
                    </children>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                </HBox>
                <HBox spacing="20.0">
                    <children>
                        <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Currency"/>
                        <TextField fx:id="currency" disable="true" editable="false" promptText="g1" text="g1"/>
                    </children>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                </HBox>
                <HBox spacing="20.0">
                    <children>
                        <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="PublicKey"/>
                        <PasswordField fx:id="salt"/>
                        <PasswordField fx:id="password"/>
                        <Label fx:id="pubkey" maxHeight="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
                    </children>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                </HBox>
                <HBox spacing="20.0">
                    <children>
                        <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Block"/>
                        <TextField fx:id="block" editable="false" prefHeight="25.0" prefWidth="400.0"/>
                    </children>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                </HBox>
                <HBox prefHeight="444.0" prefWidth="880.0" spacing="20.0" VBox.vgrow="ALWAYS">
                    <children>
                        <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308"
                                       text="Endpoints" VBox.vgrow="ALWAYS">
                                    <font>
                                        <Font size="22.0"/>
                                    </font>
                                </Label>
                                <HBox spacing="10.0">
                                    <children>
                                        <ComboBox fx:id="typeCombo" prefWidth="150.0" promptText="Type"/>
                                        <TextField fx:id="sessid" editable="false" prefHeight="25.0" prefWidth="80.0"
                                                   promptText="SessionID"/>
                                        <TextField fx:id="url" prefHeight="25.0" prefWidth="150.0" promptText="URL"
                                                   HBox.hgrow="ALWAYS"/>
                                        <TextField fx:id="ip4" prefHeight="25.0" prefWidth="150.0" promptText="IP4"/>
                                        <TextField fx:id="ip6" prefHeight="25.0" prefWidth="150.0" promptText="IP6"/>
                                        <TextField fx:id="port" prefHeight="25.0" prefWidth="80.0" promptText="Port"/>
                                        <Button alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false"
                                                onAction="#addEndPoint" text="+"
                                                HBox.hgrow="NEVER"/>
                                    </children>
                                </HBox>
                                <VBox fx:id="endpointsContainer" prefHeight="200.0" prefWidth="100.0"/>
                            </children>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </VBox>
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </HBox>
            </children>
        </VBox>
    </children>
</VBox>

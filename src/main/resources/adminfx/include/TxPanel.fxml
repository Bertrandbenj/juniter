<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="juniter.service.adminfx.include.TxPanel">
    <children>
        <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
            <children>
                <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="7.0" HBox.hgrow="ALWAYS">
                    <children>
                        <HBox spacing="20.0" VBox.vgrow="ALWAYS">
                            <children>
                                <Label alignment="CENTER_RIGHT" prefWidth="100.0" text="Version" textAlignment="RIGHT" />
                                <TextField fx:id="fieldVersion" disable="true" editable="false" text="10" />
                            </children>
                        </HBox>
                        <HBox spacing="20.0" VBox.vgrow="ALWAYS">
                            <children>
                                <Label alignment="CENTER_RIGHT" prefWidth="100.0" text="Type" textAlignment="RIGHT" />
                                <TextField disable="true" editable="false" text="Transaction" />
                            </children>
                        </HBox>
                        <HBox spacing="20.0" VBox.vgrow="ALWAYS">
                            <children>
                                <Label alignment="CENTER_RIGHT" prefWidth="100.0" text="Currency" />
                                <TextField fx:id="fieldCurrency" disable="true" editable="false" text="g1" />
                            </children>
                        </HBox>
                        <HBox spacing="20.0">
                            <children>
                                <Label alignment="CENTER_RIGHT" prefWidth="100.0" text="Blockstamp" />
                                <HBox HBox.hgrow="ALWAYS">
                                    <children>
                                        <TextField fx:id="fieldBlockstamp" disable="true" editable="false" HBox.hgrow="ALWAYS" />
                                    </children>
                                </HBox>
                            </children>
                        </HBox>
                        <HBox spacing="20.0">
                            <children>
                                <Label alignment="CENTER_RIGHT" prefWidth="100.0" text="Locktime" />
                                <TextField fx:id="fieldLocktime" disable="true" editable="false" text="0" />
                            </children>
                        </HBox>
                        <Separator prefWidth="200.0" />
                        <HBox spacing="20.0" VBox.vgrow="ALWAYS">
                            <children>
                                <Label alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" prefWidth="100.0" text="Issuers" HBox.hgrow="ALWAYS" />
                                <VBox HBox.hgrow="ALWAYS">
                                    <children>
                                        <HBox spacing="10.0" VBox.vgrow="ALWAYS">
                                            <children>
                                                <VBox layoutX="10.0" layoutY="10.0" spacing="5.0">
                                                    <children>
                                                        <TextField fx:id="issAmount" prefWidth="100.0" promptText="amount" />
                                                        <ComboBox fx:id="issUnit" disable="true" editable="true" prefWidth="100.0" promptText="Unit" />
                                                    </children>
                                                </VBox>
                                                <VBox spacing="5.0" HBox.hgrow="ALWAYS">
                                                    <children>
                                                        <PasswordField fx:id="issSalt" prefWidth="400.0" promptText="salt" VBox.vgrow="ALWAYS" />
                                                        <PasswordField fx:id="issPass" promptText="password" VBox.vgrow="ALWAYS" />
                                                    </children>
                                                </VBox>
                                                <VBox alignment="CENTER" HBox.hgrow="NEVER">
                                                    <children>
                                                        <Button mnemonicParsing="false" onAction="#addIssuers" text="+" VBox.vgrow="ALWAYS">
                                                            <font>
                                                                <Font size="24.0" />
                                                            </font>
                                                        </Button>
                                                    </children>
                                                </VBox>
                                            </children>
                                        </HBox>
                                        <VBox fx:id="issuerContainer" spacing="5.0" VBox.vgrow="ALWAYS">
                                            <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                            </padding>
                                        </VBox>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                        <Separator prefWidth="200.0" />
                        <HBox spacing="20.0">
                            <children>
                                <Label alignment="CENTER_RIGHT" prefWidth="100.0" text="Inputs" />
                        <HBox HBox.hgrow="ALWAYS">
                           <children>
                              <Button mnemonicParsing="false" onAction="#refreshInputs" text="refreshInputs" />
                                      <VBox fx:id="inputContainer" alignment="CENTER" HBox.hgrow="SOMETIMES" />
                           </children>
                        </HBox>
                            </children>
                        </HBox>
                  <Separator prefWidth="200.0" />
                        <HBox spacing="20.0">
                            <children>
                                <Label alignment="CENTER_RIGHT" prefWidth="100.0" text="Unlocks" />
                        <HBox HBox.hgrow="ALWAYS">
                           <children>
                              <Button mnemonicParsing="false" onAction="#refreshUnlocks" text="refreshUnlocks" />
                                      <VBox fx:id="unlockContainer" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                            </children>
                        </HBox>
                        <Separator prefWidth="200.0" />
                        <HBox spacing="20.0">
                            <children>
                                <Label alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" prefWidth="100.0" text="Outputs" />
                                <VBox HBox.hgrow="ALWAYS">
                                    <children>
                                        <HBox spacing="10.0">
                                            <children>
                                                <TextField fx:id="amountDest" prefWidth="100.0" promptText="amount" />
                                                <TextField fx:id="pkDest" promptText="pubkey dest" HBox.hgrow="ALWAYS" />
                                                <Button mnemonicParsing="false" onAction="#addOutputs" text="+" />
                                                <Region prefWidth="14.0" />
                                            </children>
                                        </HBox>
                                        <VBox fx:id="outputContainer" spacing="5.0" VBox.vgrow="ALWAYS">
                                            <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                            </padding>
                                        </VBox>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                        <Separator prefWidth="200.0" />
                        <HBox spacing="20.0">
                            <children>
                                <Label alignment="CENTER_RIGHT" prefHeight="27.0" prefWidth="100.0" text="Signatures" />
                                <VBox fx:id="signatureContainer" HBox.hgrow="ALWAYS" />
                            </children>
                        </HBox>
                        <Separator prefWidth="200.0" />
                        <HBox spacing="20.0">
                            <children>
                                <Label alignment="CENTER_RIGHT" prefHeight="27.0" prefWidth="100.0" text="Comment" />
                                <HBox maxWidth="400.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <TextField fx:id="fieldComment" onKeyTyped="#refresh" promptText="comment" HBox.hgrow="ALWAYS" />
                                    </children>
                                </HBox>
                            </children>
                        </HBox>
                    </children>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </padding>
                </VBox>
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </HBox>
    </children>
</AnchorPane>

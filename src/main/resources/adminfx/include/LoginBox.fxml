<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

    <HBox xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="juniter.service.adminfx.include.LoginBox"
          prefHeight="400.0" prefWidth="600.0">
        <children>
            <Region prefHeight="130.0" prefWidth="446.0" HBox.hgrow="ALWAYS" />
            <VBox alignment="CENTER" prefHeight="188.0" prefWidth="449.0" spacing="5.0" styleClass="loginBox">
                <children>
                    <Label text="Login">
                        <font>
                            <Font size="24.0" />
                        </font>
                    </Label>
                    <PasswordField fx:id="salt" onKeyTyped="#login" prefHeight="25.0" prefWidth="485.0" promptText="salt - secret key ">
                        <font>
                            <Font size="10.0" />
                        </font>
                    </PasswordField>
                    <PasswordField fx:id="password" onKeyTyped="#login" prefHeight="25.0" prefWidth="660.0" promptText="password - secret password">
                        <font>
                            <Font size="10.0" />
                        </font>
                    </PasswordField>
                    <HBox spacing="10.0">
                        <children>
                            <Button fx:id="login" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="77.0" text="connect " />
                            <Region HBox.hgrow="ALWAYS" />
                            <Label fx:id="pubkey" text="Public Key">
                                <font>
                                    <Font name="Bitstream Vera Sans Mono Bold" size="16.0" />
                                </font>
                            </Label>
                        </children>
                        <VBox.margin>
                            <Insets />
                        </VBox.margin>
                    </HBox>
                    <HBox spacing="10.0" VBox.vgrow="NEVER">
                        <children>
                            <Region HBox.hgrow="ALWAYS" />
                        </children>
                        <VBox.margin>
                            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                        </VBox.margin>
                    </HBox>
                </children>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
            </VBox>
            <Region prefHeight="130.0" prefWidth="446.0" />
        </children>
    </HBox>

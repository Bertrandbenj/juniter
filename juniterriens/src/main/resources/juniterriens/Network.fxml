<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="contentPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="juniter.juniterriens.Network">
    <right>
        <VBox alignment="CENTER" BorderPane.alignment="CENTER">
            <children>
                <fx:include fx:id="menu" source="include/Menu.fxml" />
            </children>
        </VBox>
    </right>
    <bottom>
        <fx:include source="include/Footer.fxml" />
    </bottom>

    <center>
        <ScrollPane fitToWidth="true" prefWidth="1050.0" BorderPane.alignment="CENTER">
           <content>
               <VBox BorderPane.alignment="CENTER">
                   <children>
   
                       <VBox alignment="CENTER" spacing="10.0">
                           <children>
                               <HBox alignment="CENTER_LEFT" spacing="20.0">
                                   <children>
                                       <Button mnemonicParsing="false" onAction="#peerCheck" text="peerCheck" />
                                       <Button mnemonicParsing="false" onAction="#pairing" text="Pairing" />
                                       <Button mnemonicParsing="false" onAction="#ping" text="200 pings" />
   
                                   </children>
                               </HBox>
                               <HBox spacing="15.0">
                                   <children>
                                       <TextField fx:id="tstSome" prefHeight="25.0" prefWidth="370.0" promptText="ex .. 15144,31202,85448,87566,90830,109327" />
                                       <Button mnemonicParsing="false" onAction="#testSome" text="is / are valid ? " />
                              <CheckBox fx:id="testLowerThan" mnemonicParsing="false" text="lower than" />
                              <CheckBox fx:id="testHigherThan" mnemonicParsing="false" text="higher Than" />
                                   </children>
                               </HBox>
                               <HBox spacing="15.0">
                                   <children>
                                       <TextField fx:id="delSome" prefHeight="25.0" prefWidth="370.0" promptText="ex .. 15144,31202,85448,87566,90830,109327" />
                                       <Button mnemonicParsing="false" onAction="#deleteSome" text="delete ! " />
                              <CheckBox fx:id="delLowerThan" mnemonicParsing="false" text="lower than" />
                              <CheckBox fx:id="delHigherThan" mnemonicParsing="false" text="higher Than" />
                                   </children>
                               </HBox>
                           </children>
                           <padding>
                               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                           </padding>
                       </VBox>
                       <HBox VBox.vgrow="ALWAYS">
                           <children>
                               <StackedAreaChart fx:id="pow" title="Pow" HBox.hgrow="ALWAYS">
                                   <xAxis>
                                       <NumberAxis fx:id="powX" autoRanging="false" side="BOTTOM" />
                                   </xAxis>
                                   <yAxis>
                                       <NumberAxis fx:id="powY" autoRanging="false" side="LEFT" />
                                   </yAxis>
                               </StackedAreaChart>
                               <LineChart fx:id="issuersFrameVar" title="IssuersFrameVar" HBox.hgrow="ALWAYS">
                                   <xAxis>
                                       <NumberAxis fx:id="issuersFrameVarX" autoRanging="false" side="BOTTOM" />
                                   </xAxis>
                                   <yAxis>
                                       <NumberAxis fx:id="issuersFrameVarY" side="LEFT" />
                                   </yAxis>
                               </LineChart>
                           </children>
                       </HBox>
                       <HBox VBox.vgrow="ALWAYS">
                           <children>
                               <LineChart fx:id="medianTime" title="MedianTime" HBox.hgrow="ALWAYS">
                                   <xAxis>
                                       <NumberAxis fx:id="medianX" autoRanging="false" side="BOTTOM" />
                                   </xAxis>
                                   <yAxis>
                                       <NumberAxis fx:id="medianY" side="LEFT" />
                                   </yAxis>
                               </LineChart>
                               <LineChart fx:id="issuersFrame" title="Issuers  Frame" HBox.hgrow="ALWAYS">
                                   <xAxis>
                                       <NumberAxis fx:id="issuersFrameX" autoRanging="false" side="BOTTOM" />
                                   </xAxis>
                                   <yAxis>
                                       <NumberAxis fx:id="issuersFrameY" side="LEFT" />
                                   </yAxis>
                               </LineChart>
                           </children>
                       </HBox>
                       <HBox VBox.vgrow="ALWAYS">
                           <children>
                               <PieChart fx:id="netstats" legendSide="LEFT" title="&quot;next node&quot; probability " HBox.hgrow="ALWAYS" />
   
                           </children>
                       </HBox>
                       <FlowPane fx:id="peersList" hgap="10.0" prefHeight="108.0" prefWidth="980.0" vgap="10.0">
                           <children>

                        </children>
                           <VBox.margin>
                               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                       </FlowPane>
                   </children>
               </VBox>
           </content>
        </ScrollPane>
    </center>
</BorderPane>

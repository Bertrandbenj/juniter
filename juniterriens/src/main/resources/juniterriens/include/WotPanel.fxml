<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"

            fx:controller="juniter.juniterriens.include.WotPanel">
   <children>
       <Group>
           <children>
               <VBox>
                   <children>
                       <VBox >
                           <children>
                               <HBox spacing="20.0">
                                   <children>
                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Version" />
                                       <TextField fx:id="version" editable="false" prefHeight="25.0" prefWidth="120.0" text="10" />
                                   </children>
                                   <padding>
                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                   </padding>
                               </HBox>
                               <HBox spacing="20.0">
                                   <children>
                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Type" />
                                       <RadioButton fx:id="swIdty" mnemonicParsing="false" onAction="#switchIdty" selected="true" text="Identity">
                                           <padding>
                                               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                           </padding>
                                           <toggleGroup>
                                               <ToggleGroup fx:id="wotType" />
                                           </toggleGroup>
                                       </RadioButton>
                                       <RadioButton fx:id="swMember" mnemonicParsing="false" onAction="#switchMembership" text="Membership" toggleGroup="$wotType">
                                           <padding>
                                               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                           </padding>
                                       </RadioButton>
                                       <RadioButton fx:id="swCertif" mnemonicParsing="false" onAction="#switchCertif" text="Certification" toggleGroup="$wotType">
                                           <padding>
                                               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                           </padding>
                                       </RadioButton>
                              <RadioButton fx:id="swRevoc" mnemonicParsing="false" onAction="#switchRevoc" text="Revocation" toggleGroup="$wotType">
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </RadioButton>
                                   </children>
                                   <padding>
                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                   </padding>
                               </HBox>
                               <HBox spacing="20.0">
                                   <children>
                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Currency" />
                                       <TextField fx:id="currency" editable="false" prefHeight="25.0" prefWidth="120.0" promptText="g1" text="g1" HBox.hgrow="NEVER" />
                                   </children>
                                   <padding>
                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                   </padding>
                               </HBox>
                               <HBox spacing="20.0">
                                   <children>
                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Issuer" />
                              <VBox spacing="5.0">
                                 <children>
                                     <PasswordField fx:id="salt" onAction="#refresh" promptText="salt" />
                                    <PasswordField fx:id="password" onAction="#refresh" promptText="password" />
                                 </children>
                              </VBox>
                              <Label fx:id="pk" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="pubkey" HBox.hgrow="ALWAYS" />
                                   </children>
                                   <padding>
                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                   </padding>
                               </HBox>
                                   <Separator prefWidth="200.0" />
                               <HBox VBox.vgrow="ALWAYS">
                                   <children>
                                       <VBox fx:id="boxIdty" minWidth="0.0" prefWidth="404.0" HBox.hgrow="ALWAYS">
                                           <children>
                                               <HBox spacing="20.0">
                                                   <children>
                                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="UniqueID" />
                                                       <TextField fx:id="uniqueID" onAction="#refresh" HBox.hgrow="ALWAYS" />
                                                   </children>
                                                   <padding>
                                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                               </HBox>
                                               <HBox spacing="20.0">
                                                   <children>
                                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Timestamp" />
                                                       <TextField fx:id="timestamp" HBox.hgrow="ALWAYS" />
                                                   </children>
                                                   <padding>
                                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                               </HBox>
                                           </children>
                                       </VBox>
                                       <VBox fx:id="boxMembership" minWidth="0.0" HBox.hgrow="ALWAYS">
                                           <children>
                                               <HBox spacing="20.0">
                                                   <children>
                                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Block" />
                                                       <TextField fx:id="block" editable="false" HBox.hgrow="ALWAYS" />
                                                   </children>
                                                   <padding>
                                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                               </HBox>
                                               <HBox spacing="20.0">
                                                   <children>
                                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Membership" />
                                                       <TextField disable="true" editable="false" maxWidth="100.0" text="IN" />
                                                   </children>
                                                   <padding>
                                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                               </HBox>
                                               <HBox spacing="20.0">
                                                   <children>
                                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="UserID" />
                                                       <TextField fx:id="userid" onKeyTyped="#refresh" HBox.hgrow="ALWAYS" />
                                                   </children>
                                                   <padding>
                                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                               </HBox>
                                               <HBox spacing="20.0">
                                                   <children>
                                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="CertTS" />
                                                       <TextField fx:id="certTS" HBox.hgrow="ALWAYS" />
                                                   </children>
                                                   <padding>
                                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                               </HBox>
                                           </children>
                                       </VBox>
                                       <VBox fx:id="boxCertification" minWidth="0.0" prefWidth="500.0" HBox.hgrow="ALWAYS">
                                           <children>
                                               <HBox spacing="20.0">
                                                   <children>
                                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="IdtyIssuer" />
                                                       <TextField fx:id="idtyIssuer" onKeyTyped="#refresh" HBox.hgrow="ALWAYS" />
                                                   </children>
                                                   <padding>
                                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                               </HBox>
                                               <HBox spacing="20.0">
                                                   <children>
                                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="IdtyUniqueID" />
                                                       <TextField fx:id="idtyUniqueID" onKeyTyped="#refresh" HBox.hgrow="ALWAYS" />
                                                   </children>
                                                   <padding>
                                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                               </HBox>
                                               <HBox spacing="20.0">
                                                   <children>
                                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="IdtyTimestamp" />
                                                       <TextField fx:id="idtyTimestamp" editable="false" HBox.hgrow="ALWAYS" />
                                                   </children>
                                                   <padding>
                                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                               </HBox>
                                               <HBox spacing="20.0">
                                                   <children>
                                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="IdtySignature" />
                                                       <TextField fx:id="idtySignature" HBox.hgrow="ALWAYS" />
                                                   </children>
                                                   <padding>
                                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                               </HBox>
                                               <HBox spacing="20.0">
                                                   <children>
                                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="CertTimestamp" />
                                                       <TextField fx:id="certTimestamp" editable="false" HBox.hgrow="ALWAYS" />
                                                   </children>
                                                   <padding>
                                                       <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                               </HBox>
                                           </children>
                                       </VBox>
                              <VBox fx:id="boxRevocation" minWidth="0.0" prefWidth="500.0">
                                 <children>
                                    <HBox spacing="20.0">
                                       <children>
                                          <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="IdtyUniqueID" />
                                          <TextField fx:id="idtyUniqueIDRev" onKeyTyped="#refresh" HBox.hgrow="ALWAYS" />
                                       </children>
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding>
                                    </HBox>
                                    <HBox spacing="20.0">
                                       <children>
                                          <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="IdtyTimestamp" />
                                          <TextField fx:id="idtyTimestampRev" editable="false" HBox.hgrow="ALWAYS" />
                                       </children>
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding>
                                    </HBox>
                                    <HBox spacing="20.0">
                                       <children>
                                          <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="IdtySignature" />
                                          <TextField fx:id="idtySignatureRev" onKeyTyped="#refresh" HBox.hgrow="ALWAYS" />
                                       </children>
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding>
                                    </HBox>
                                 </children>
                              </VBox>
                                   </children>
                               </HBox>
                                   <Separator prefWidth="200.0" />
                               <HBox spacing="20.0" VBox.vgrow="ALWAYS">
                                   <children>
                                       <Label alignment="CENTER" prefHeight="27.0" prefWidth="149.0" text="Signature" />
                                       <TextField fx:id="signature" editable="false" prefWidth="400.0" />
                                   </children>
                                   <padding>
                                       <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                   </padding>
                               </HBox>
                           </children>
                       </VBox>
                       <HBox alignment="CENTER">
                           <padding>
                               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                       </HBox>
                   </children>
                   <padding>
                       <Insets bottom="10.0" top="10.0" />
                   </padding>
               </VBox>
           </children>
       </Group>
   </children>
</AnchorPane>

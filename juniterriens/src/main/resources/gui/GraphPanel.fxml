<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.web.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.WebView?>

<BorderPane styleClass="pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="juniter.gui.GraphPanel">
    <right>

                <fx:include fx:id="menu" source="include/Menu.fxml" />


    </right>

    <bottom>

        <fx:include source="include/Footer.fxml" />
    </bottom>
    <center>
        <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <tabs>
                <Tab text="GraphViz">
                    <content>
                        <WebView fx:id="SVGAnchor" prefHeight="200.0" prefWidth="200.0" />
                     </content>
                </Tab>
                <Tab text="Cesium">
                    <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                            <image>

                                <Image url="@images/cesium.png" />
                            </image>
                            <viewport>
                                <Rectangle2D />
                             </viewport>
                        </ImageView>
                    </graphic>
                    <content>

                        <WebView fx:id="CesiumAnchor" prefHeight="200.0" prefWidth="200.0" />
                     </content>

                </Tab>
                <Tab text="WotMap">
                    <content>

                        <WebView fx:id="WotMapAnchor" prefHeight="200.0" prefWidth="200.0" />
                     </content>
                </Tab>
                <Tab fx:id="huhu">
                    <content>

                        <WebView fx:id="SVGAnchor111" prefHeight="200.0" prefWidth="200.0" />
                    </content>
                    <graphic>
                        <TextField fx:id="uri" alignment="CENTER" onAction="#go" prefHeight="24.0" prefWidth="250.0" promptText="https://forum.monnaie-libre.fr/" />
                     </graphic>
                </Tab>
            </tabs>
        </TabPane>
    </center>
</BorderPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<VBox maxWidth="130.0" styleClass="pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="juniter.gui.business.panel.Menu">
    <children>
        <VBox fx:id="vMenu" alignment="CENTER" VBox.vgrow="ALWAYS">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <children>
                <Region VBox.vgrow="ALWAYS"/>
                <Hyperlink onAction="#viewMain">
                    <graphic>
                        <ImageView fx:id="logoMain" fitHeight="60.0" fitWidth="72.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../images/whiterabbit.jpg"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="%menu.tooltip.front"/>
                    </tooltip>
                </Hyperlink>
                <Hyperlink onAction="#viewSVGGraph">
                    <graphic>
                        <ImageView fx:id="logoGraphs" fitHeight="60.0" fitWidth="72.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../images/logo.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="%menu.tooltip.graph"/>
                    </tooltip>
                </Hyperlink>
                <Hyperlink onAction="#viewNotary">
                    <graphic>
                        <ImageView fx:id="logoNotary" fitHeight="60.0" fitWidth="72.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../images/keep_calm_im_the_notary_puzzle.jpg"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="%menu.tooltip.notary"/>
                    </tooltip>
                </Hyperlink>
                <Hyperlink onAction="#viewNetwork">
                    <graphic>
                        <ImageView fx:id="logoNetwork" fitHeight="60.0" fitWidth="72.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../images/network.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="%menu.tooltip.network"/>
                    </tooltip>
                </Hyperlink>
                <Hyperlink onAction="#viewCurrencies">
                    <graphic>
                        <ImageView fx:id="logoCurrencies" fitHeight="60.0" fitWidth="72.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../images/g1.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="%menu.tooltip.network"/>
                    </tooltip>
                </Hyperlink>
                <Hyperlink onAction="#viewDatabase">
                    <graphic>
                        <ImageView fx:id="logoDatabase" fitHeight="60.0" fitWidth="72.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../images/database.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="%menu.tooltip.database"/>
                    </tooltip>
                </Hyperlink>
                <Hyperlink onAction="#viewSpark" id="sparkMenuButton">
                    <graphic>
                        <ImageView fx:id="logoSpark" fitHeight="60.0" fitWidth="72.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../images/spark.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="%menu.tooltip.spark"/>
                    </tooltip>
                </Hyperlink>
                <Hyperlink onAction="#viewSettings">
                    <graphic>
                        <ImageView fx:id="logoSettings" fitHeight="60.0" fitWidth="72.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../images/settings.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="%menu.tooltip.settings"/>
                    </tooltip>
                </Hyperlink>

                <Region VBox.vgrow="ALWAYS"/>
            </children>
        </VBox>
    </children>
</VBox>
